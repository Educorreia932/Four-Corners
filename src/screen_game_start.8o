# Macro to draw a sprite
:macro draw A {
	i := A
	x := x_pos
	y := y_pos

	loop
		sprite y x 0

		x_target := x_pos
		x_target += length

		y_target := y_pos
		y_target += height
		
		i += stride
		x += 16

		if x == x_target then y += 16
		if x == x_target then x := x_pos
		if y != y_target then
	again
}

# Macro to render music bytes
:macro def-note NAME PITCH { : NAME vf := PITCH jump note }
def-note B-4 159
def-note A$4 155
def-note G$4 147
def-note D$4 127
def-note D-4 123
def-note C$4 119
def-note C-4 115

: jump_tone A$4 ;
: waveform  0x00 0x00 0xFF 0xFF 0x00 0x00 0xFF 0xFF 0x00 0x00 0xFF 0xFF 0x00 0x00 0xFF 0xFF
: silence   0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00

: ---
	i := silence
	jump play
: note
	pitch := vf
	i := waveform
: play
	audio
	vf     := 6   # actual note duration
	delay  := vf
	buzzer := vf
	loop
		vf := delay
		if vf != 0 then
	again
	jump jump_player

# DEFAULT SPEED 20 CYCLES/FRAME
:alias counter	v5
:alias stride		v6
:alias x	    	v7
:alias y      		v8
:alias x_pos  		v9
:alias y_pos  		va
:alias x_target 	vb
:alias y_target 	vc
:alias length		vd
:alias height		ve

###########################################
#
#  Introduction screen
#
###########################################

: main
	render_background
	render_player
	
: render_background
	hires
	plane 3
	stride := 64
	length := 64
	height := 128
	draw background

: render_player
	x_pos := 20
	y_pos := 67
	length := 32
	height := 32
	draw pig
	
    loop
				: movement_player
				draw pig
        v0 := OCTO_KEY_A if v0 key then x_pos += 1
				v0 := OCTO_KEY_W if v0 key begin
						jump_tone
						: jump_player
						counter := 0
						loop
							counter += 1
							if counter <= 10 then y_pos -= 1
							if counter > 10 then y_pos += 1
							draw pig
							draw pig
							if counter != 20 then
						again
				end
        v0 := OCTO_KEY_D if v0 key then x_pos += -1
        draw pig
					loop
						vf := delay
						if vf != 0 then
    			again
    		vf := 10
    		delay := vf
				: options_game
				if x_pos >= 30 begin
					if x_pos <= 40 begin
				# render four corners icon #
				x := 45
				y := 40
				i := four_corners_icon
				sprite y x 0
				############################
				end end
  	again
