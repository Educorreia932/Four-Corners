###########################################
#
#  Macros
#
###########################################
# DEFAULT SPEED 20 CYCLES/FRAME
:alias counter	v5
:alias stride		v6
:alias x	    	v7
:alias y      		v8
:alias x_pos  		v9
:alias y_pos  		va
:alias x_target 	vb
:alias y_target 	vc
:alias length		vd
:alias height		ve

# Macro to draw a sprite
:macro draw A {
	i := A
	x := x_pos
	y := y_pos

	loop
		sprite y x 0

		x_target := x_pos
		x_target += length

		y_target := y_pos
		y_target += height
		
		i += stride
		x += 16

		if x == x_target then y += 16
		if x == x_target then x := x_pos
		if y != y_target then
	again
}

# Macro to render music bytes
: waveform  0x00 0x00 0xFF 0xFF 0x00 0x00 0xFF 0xFF 0x00 0x00 0xFF 0xFF 0x00 0x00 0xFF 0xFF

: jump_tone
vf := 155 pitch := vf counter := 1
i := waveform
jump play

: select_tone 
vf := 147 pitch := vf counter := 2
i := waveform
jump play

: play
	audio
	vf     := 6   # actual note duration
	delay  := vf
	buzzer := vf
	loop
		vf := delay
		if vf != 0 then
	again
	if counter == 1 then
		jump jump_player
	if counter == 2 then
		jump start_game


###########################################
#
#  Introduction screen
#
###########################################

: main
	
: render_background
	hires
	plane 3
	stride := 64
	length := 64
	height := 128
	draw background

: render_player
	x_pos := 20
	y_pos := 67
	length := 32
	height := 32
	draw pig
	
    loop
	: movement_player
	draw pig
        v0 := OCTO_KEY_A if v0 key then x_pos += 1
	v0 := OCTO_KEY_W if v0 key begin jump_tone
		: jump_player
		counter := 0
		loop
			counter += 1
			if counter <= 10 then y_pos -= 1
			if counter > 10 then y_pos += 1
			draw pig
			draw pig
			if counter != 20 then
		again
	end
        v0 := OCTO_KEY_D if v0 key then x_pos += -1
        draw pig
	loop
		vf := delay
		if vf != 0 then
    	again
    		vf := 10
    		delay := vf
		: options_game
		if x_pos >= 30 begin
			if x_pos <= 40 begin
				# render four corners icon #
				x := 45
				y := 40
				i := four_corners_icon
				sprite y x 0
				############################
				v0 := OCTO_KEY_S if v0 key begin select_tone
					jump start_game end
				end end
  	again
		
: start_game
	clear
	
	x_pos := 0
	y_pos := 0
	stride := 64
	length := 64
	height := 128
	draw background
	
	: render_splash_screen
	x_pos := 8
	y_pos := 45
	length := 48
	height := 16
	draw four-corners_header-text