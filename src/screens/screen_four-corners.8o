# Macro to draw a sprite
:macro draw A {
	i := A
	x := x_pos
	y := y_pos

	loop
		sprite y x 0

		x_target := x_pos
		x_target += length

		y_target := y_pos
		y_target += height
		
		i += stride
		x += 16

		if x == x_target then y += 16
		if x == x_target then x := x_pos
		if y != y_target then
	again
}

:macro draw_cat {
	x_pos := 34
	y_pos := 32
	length := 32
	height := 32

	draw cat
}

:macro draw_heart {
	x_pos := 4
	y_pos := 36
	length := 32
	height := 32

	draw heart
}

:macro draw_zebra {
	x_pos := 35
	y_pos := 66
	length := 32
	height := 32

	draw zebra
}

:macro draw_pig {
	x_pos := 4
	y_pos := 68
	length := 32
	height := 32

	draw pig
}

:alias turn_counter v3
:alias cpu_selected v4
:alias stride		v6
:alias x	    	v7
:alias y      		v8
:alias x_pos  		v9
:alias y_pos  		va
:alias x_target 	vb
:alias y_target 	vc
:alias length		vd
:alias height		ve

: main
	hires  # Set high resolution mode
	plane 3
	# draw title
	stride := 64
	length := 64
	height := 128
	draw background
	
	draw_cat
	draw_heart
	draw_zebra
	draw_pig
	
	turn_counter := 0
	x := 0
	i := dot
	
	loop
		turn_counter += 1

		cpu_selected := random 3
		if cpu_selected == 0 begin y := 64 x := 61 end
		if cpu_selected == 1 begin y := 0 x := 61 end
		if cpu_selected == 2 begin y := 64 x := 61 end
		if cpu_selected == 3 begin y := 0 x := 61 end

		# Main game loop		
		loop
			if cpu_selected == 0 begin
				sprite x y 1
				if y > 32 then y -= 3
				if y < 32 then x -= 3
				if x >= 32 begin
					again
				else
					draw_cat
				end
			end
		loop
			if cpu_selected == 1 begin
				sprite x y 1
				if y < 32 then y += 3
				if y > 32 then x -= 3
				if x >= 32 begin
					again
				else
					draw_heart
				end
			end
		loop
			if cpu_selected == 2 begin
				sprite x y 1
				if y > 32 then y -= 3
				if y < 32 then x += 3
				if x <= 93 begin
					again
				else
					draw_zebra
				end
			end
		loop
			if cpu_selected == 3 begin
				sprite x y 1
				if y < 32 then y += 3
				if y > 32 then x += 3
				if x <= 93 begin
					again
				else
					draw_pig
				end
			end
			
			if turn_counter < 3 then 
		again