:macro draw A {
	i := A
	x := x_pos
	y := y_pos
	loop
		sprite y x 0
		x_target := x_pos
		x_target += length
		y_target := y_pos
		y_target += height
		i += stride
		x += 16
		if x == x_target then y += 16
		if x == x_target then	x := x_pos
		if y != y_target then
	again
	}

:alias counter	v5
:alias stride		v6
:alias x	    	v7
:alias y      		v8
:alias x_pos  		v9
:alias y_pos  		va
:alias x_target 	vb
:alias y_target 	vc
:alias length		vd
:alias height		ve

: main
	hires
	plane 3
	# draw title
	stride := 64
	length := 64
	height := 128
	draw background

	x_pos := 20
	y_pos := 52
	length := 32
	height := 32
	draw pig

    loop
				draw pig
        v0 := OCTO_KEY_A if v0 key then x_pos += 1
				v0 := OCTO_KEY_W if v0 key begin
						counter := 0
						loop
							counter += 1
							draw pig
							if counter <= 3 then y_pos -= 1
							if counter > 3 then y_pos += 1
							if counter != 6 then
						again
				end
        v0 := OCTO_KEY_D if v0 key then x_pos += -1
        draw pig
					loop
						vf := delay
						if vf != 0 then
    			again
    		vf := 3
    		delay := vf
  	again